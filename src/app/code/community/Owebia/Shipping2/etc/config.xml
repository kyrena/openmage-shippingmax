<config>
    <modules>
        <Owebia_Shipping2>
            <version>2.6.10</version>
            <lite>true</lite>
        </Owebia_Shipping2>
    </modules>
    <global>
        <models>
            <owebia_shipping2>
                <class>Owebia_Shipping2_Model</class>
            </owebia_shipping2>
        </models>
        <blocks>
            <owebia_shipping2>
                <class>Owebia_Shipping2_Block</class>
            </owebia_shipping2>
        </blocks>
        <helpers>
            <owebia_shipping2>
                <class>Owebia_Shipping2_Helper</class>
            </owebia_shipping2>
        </helpers>
        <resources>
            <sales>
                <shipping>
                    <carriers>
                        <owebiashipping1>
                            <class>Owebia_Shipping2_Model_Carrier_OwebiaShipping1</class>
                        </owebiashipping1>
                        <owebiashipping2>
                            <class>Owebia_Shipping2_Model_Carrier_OwebiaShipping2</class>
                        </owebiashipping2>
                        <owebiashipping3>
                            <class>Owebia_Shipping2_Model_Carrier_OwebiaShipping3</class>
                        </owebiashipping3>
                    </carriers>
                </shipping>
            </sales>
        </resources>
    </global>
    <adminhtml>
        <translate>
            <modules>
                <Owebia_Shipping2>
                    <files>
                        <default>Owebia_Shipping2.csv</default>
                    </files>
                </Owebia_Shipping2>
            </modules>
        </translate>
    </adminhtml>
    <default>
        <owebia_shipping2>
            <general>
                <weight_unit>kg</weight_unit>
            </general>
            <bundle_product>
                <process_children>0</process_children>
                <load_item_options_on_parent>1</load_item_options_on_parent>
                <load_item_data_on_parent>1</load_item_data_on_parent>
                <load_product_data_on_parent>1</load_product_data_on_parent>
            </bundle_product>
            <configurable_product>
                <load_item_options_on_parent>1</load_item_options_on_parent>
                <load_item_data_on_parent>1</load_item_data_on_parent>
                <load_product_data_on_parent>0</load_product_data_on_parent>
            </configurable_product>
        </owebia_shipping2>
        <carriers>
            <owebiashipping1>
                <active>0</active>
                <title>Owebia 1</title>
                <model>Owebia_Shipping2_Model_Carrier_OwebiaShipping1</model>
                <config>{
   "demo": {
      "label": "Example",
      "fees": 10
   }
}</config>
                <debug>0</debug>
                <auto_escaping>0</auto_escaping>
                <auto_correction>1</auto_correction>
                <stop_to_first_match>0</stop_to_first_match>
                <sort_order>1</sort_order>
            </owebiashipping1>
            <owebiashipping2>
                <active>0</active>
                <title>Owebia 2</title>
                <model>Owebia_Shipping2_Model_Carrier_OwebiaShipping2</model>
                <config>{
   "demo": {
      "label": "Example",
      "fees": 10
   }
}</config>
                <debug>0</debug>
                <auto_escaping>0</auto_escaping>
                <auto_correction>1</auto_correction>
                <stop_to_first_match>0</stop_to_first_match>
                <sort_order>1</sort_order>
            </owebiashipping2>
            <owebiashipping3>
                <active>0</active>
                <title>Owebia 3</title>
                <model>Owebia_Shipping2_Model_Carrier_OwebiaShipping3</model>
                <config>{
   "demo": {
      "label": "Example",
      "fees": 10
   }
}</config>
                <debug>0</debug>
                <auto_escaping>0</auto_escaping>
                <auto_correction>1</auto_correction>
                <stop_to_first_match>0</stop_to_first_match>
                <sort_order>1</sort_order>
            </owebiashipping3>
        </carriers>
    </default>
</config>